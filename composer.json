{
  "name": "jonston/symfony-sanctum-bundle",
  "description": "A Symfony bundle that provides Laravel Sanctum-like personal access token authentication",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene",
      "email": "eugene@example.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "symfony/framework-bundle": "^6.0|^7.0",
    "symfony/security-bundle": "^6.0|^7.0",
    "doctrine/orm": "^2.14|^3.0",
    "doctrine/doctrine-bundle": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0",
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Jonston\\SanctumBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jonston\\SanctumBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}