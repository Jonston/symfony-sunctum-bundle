{
  "name": "jonston/symfony-sanctum-bundle",
  "type": "symfony-bundle",
  "description": "Простая token-based аутентификация для Symfony, вдохновленная Laravel Sanctum. Обеспечивает легкую аутентификацию через API токены без сложностей OAuth.",
  "keywords": ["symfony", "authentication", "api", "token", "sanctum"],
  "license": "MIT",
  "authors": [
    {
      "name": "Your Name",
      "email": "your.email@example.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.0|^7.0",
    "symfony/security-bundle": "^6.0|^7.0",
    "doctrine/orm": "^2.0",
    "doctrine/doctrine-bundle": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Jonston\\SanctumBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jonston\\SanctumBundle\\Tests\\": "tests/"
    }
  }
}