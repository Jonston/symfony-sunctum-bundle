{
  "name": "jonston/symfony-sanctum-bundle",
  "version": "0.1.0-alpha",
  "type": "symfony-bundle",
  "description": "Simple token-based authentication for Symfony, inspired by Laravel Sanctum. Provides easy authentication via API tokens without the complexity of OAuth.",
  "keywords": ["symfony", "authentication", "api", "token", "sanctum"],
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene",
      "email": "jonston85@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.0|^7.0",
    "symfony/security-bundle": "^6.0|^7.0",
    "doctrine/orm": "^3.0",
    "doctrine/doctrine-bundle": "^2.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Jonston\\SanctumBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jonston\\SanctumBundle\\Tests\\": "tests/"
    }
  }
}